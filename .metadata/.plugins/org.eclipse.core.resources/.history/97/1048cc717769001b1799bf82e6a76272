public class TestArray {
	public static void main(String[] args) {
		int[] c = new int[5];
		c[0]= 10;
		c[1]= 20;
		c[2]= 30;
		c[3]= 40;
		System.out.println(c[2]);
		for (int i= 0; i<c.length; i++) {
			
		}
		System.out.println(Arrays.toString(c));
	}

}
